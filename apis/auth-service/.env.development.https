# Development Environment with HTTPS
NODE_ENV=development
HTTPS_ENABLED=true
HTTPS_PORT=3443

# Auth Service
PORT=3001
SESSION_SECRET=dev_session_secret_change_in_production

# Keycloak Configuration
KEYCLOAK_BASE_URL=http://localhost:8080/realms/myrealm
KEYCLOAK_CLIENT_ID=myclient
KEYCLOAK_CLIENT_SECRET=

# CORS Origins (HTTPS ready)
CLIENT_ORIGIN=https://localhost:4200
ALLOWED_ORIGINS=https://localhost:4200,https://localhost:4201,https://localhost:3443,https://localhost:3444

# Logging
LOG_LEVEL=info
